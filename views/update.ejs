<%- include partials/head.ejs %>
<h1>Update:</h1>
<form action="/updated/<%= userToUpdate.id %>" method="post">
      <label for="name">Name:</label>
      <input type="text" name="name" value="<%= userToUpdate.name %>">
      <label for="number">Number:</label>
      <input type="text" name="number" value="<%= userToUpdate.number %>">
      <input type="submit" name="submit">
</form>
